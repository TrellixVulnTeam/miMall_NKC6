</loading>\n    <div class=\"form\" v-html=\"content\"></div>\n  </div>\n</template>\n<script>\n  import Loading from './../components/Loading'\n  export default{\n    name:'alipay',\n    components:{\n      Loading\n    },\n    data(){\n      return {\n        orderId:this.$route.query.orderId,\n        content:'',\n        loading:true\n      }\n    },\n    mounted(){\n      this.paySubmit();\n    },\n    methods:{\n      paySubmit(){\n        this.axios.post('/pay',{\n          orderId:this.orderId,\n          orderName:'Vue高仿小米商城',\n          amount:0.01,//单位元\n          payType:1 //1支付宝，2微信\n        }).then((res)=>{\n          this.content = res.content;\n          setTimeout(()=>{\n            document.forms[0].submit();\n          },100)\n        })\n      }\n    }\n  }\n</script>"],"sourceRoot":"src/pages"}]}